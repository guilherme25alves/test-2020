,KARA;AAUA,IAAA,eAVA,2BAUA,QAVA,EAUA;AAAA;;AACA,MAAA,OAAA,CAAA,OAAA,CAAA,QAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,SAAA,GAAA,QAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA,CAAA,WAAA;AACA,OALA;AAMA,KAjBA;AAmBA,IAAA,cAnBA,0BAmBA,QAnBA,EAmBA;AAAA;;AACA,MAAA,OAAA,CAAA,mBAAA,CAAA,QAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,WAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA;AACA,UAAA,OAAA,CAAA,UAAA,GAAA,MAAA,CAAA,eAAA,CAAA,MAAA,CAAA,QAAA,EAAA,YAAA,EAAA,OAAA,CAAA,UAAA,EAAA,IAAA;AACA,UAAA,OAAA,CAAA,SAAA,GAAA,MAAA,CAAA,MAAA,CAAA,KAAA;AACA,SAJA;AAKA,OATA,EAUA,KAVA,CAUA,YAAA;AACA,QAAA,MAAA,CAAA,kBAAA,GAAA,IAAA;AACA,OAZA;AAaA,KAjCA;AAmCA,IAAA,QAnCA,oBAmCA,OAnCA,EAmCA;AACA,UAAA,SAAA,GAAA,OAAA,KAAA,EAAA,GAAA,WAAA,GAAA,EAAA;AACA,aAAA,SAAA;AACA,KAtCA;AAwCA,IAAA,eAxCA,6BAwCA;AAAA;;AACA,MAAA,QAAA,CAAA,YAAA,GAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA,KA5CA;AA8CA,IAAA,eA9CA,2BA8CA,KA9CA,EA8CA,GA9CA,EA8CA,KA9CA,EA8CA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,KAAA,GAAA,GAAA,KAAA,YAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,UAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,SAAA;;AACA,YAAA,KAAA,KAAA,KAAA,EAAA;AACA,iBAAA,KAAA,CAAA,CAAA,CAAA;AACA;AACA;;AACA,aAAA,IAAA;AACA;AAtDA;AAvBA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"container\"> \r\n            <div class=\"hero-sm\">\r\n                <div class=\"hero-bod\">\r\n                    <h1 class=\"title-h\">{{ title }} {{ course.title }}</h1>\r\n                </div>\r\n            </div>\r\n\r\n                <div class=\"column col-8 col-mx-auto\">\r\n                  <div class=\"filter-body columns\">\r\n                    <div class=\"column col-xs-12 col-sm-12 col-md-12 col-6 filter-item\" data-tag=\"tag-2\">\r\n                      <div class=\"card\">\r\n                        <div class=\"card-header\">\r\n                          <div class=\"card-title text-bold\">Título</div>\r\n                          <div class=\"card-subtitle text-gray\">{{ course.title }}</div>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"column col-xs-12 col-sm-12 col-md-12 col-6 filter-item\" data-tag=\"tag-3\">\r\n                      <div class=\"card\">\r\n                        <div class=\"card-header\">\r\n                          <div class=\"card-title text-bold\">Descrição</div>\r\n                          <div class=\"card-subtitle text-gray\">{{ course.description }}</div>\r\n                        </div>\r\n                      </div>\r\n                    </div>                                      \r\n                  </div>\r\n                </div>\r\n\r\n                <div class=\"columns\">\r\n                    <div class=\"column col-8 col-mx-auto col-xs-12 col-sm-12 col-md-12\">\r\n                            <table class=\"table\">\r\n                                <thead>\r\n                                    <th>Matrícula ID</th>\r\n                                    <th>Aluno</th>\r\n                                    <th>Curso</th>\r\n                                    <th>Data Matrícula</th>                                \r\n                                </thead>\r\n                                <tbody>\r\n                                    <tr v-for=\"enrollment of enrollments\" :key=\"enrollment.enrollment_id\" > \r\n                                        <td>{{ enrollment.enrollment_id }}</td>\r\n                                        <td>{{ enrollment.student_id }}</td>\r\n                                        <td>{{ enrollment.course_id }}</td>\r\n                                        <td>{{ enrollment.enrollment_date }}</td>                                    \r\n                                    </tr>\r\n                                </tbody>\r\n                            </table>\r\n                            <div v-if=\"isEmptyEnrollments\">\r\n                                <p>Este curso não possui matrículas ativas!</p>\r\n                            </div>\r\n\r\n                    </div>                \r\n                </div>\r\n\r\n                <button v-on:click=\"goListCourses\" type=\"button\" class=\"btn btn-detail\">Voltar</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Courses from '../../services/courses'\r\nimport Students from '../../services/students'\r\n\r\nexport default {\r\n    \r\n    data(){\r\n        return {           \r\n            course: {\r\n                course_id: '',\r\n                title: '',\r\n                description: '',\r\n            },            \r\n            enrollments: [],\r\n            students: [],\r\n            title: \"Detalhes Curso : \",\r\n            isEmptyEnrollments: false            \r\n        }\r\n    },\r\n\r\n    mounted(){\r\n        var course_id = this.$route.params.course_id\r\n        this.getByIdentifier(course_id)  \r\n        this.getListStudents()        \r\n        this.getEnrollments(course_id)        \r\n    },\r\n\r\n    methods:{\r\n\r\n        alert() {\r\n            this.$swal(\"Atualizado!\", \"Transação efetuada com sucesso!\", \"success\")\r\n        },\r\n\r\n        goListCourses(){\r\n            this.$router.push('/courses') \r\n        },\r\n\r\n        getByIdentifier(courseId){\r\n            Courses.getById(courseId)\r\n                .then(response =>{\r\n                    this.course.course_id = courseId\r\n                    this.course.title = response.data.data.title\r\n                    this.course.description = response.data.data.description                                  \r\n                })\r\n        },\r\n\r\n        getEnrollments(courseId){\r\n            Courses.enrollmentsByCourse(courseId)\r\n                .then( response =>{\r\n                    console.log(response.data)\r\n                    this.enrollments = response.data.data   \r\n                    this.enrollments.forEach(element => {\r\n                        //console.log(element.course_id)\r\n                        element.student_id = this.findObjectByKey(this.students , \"student_id\" , element.student_id).name\r\n                        element.course_id = this.course.title\r\n                    });                 \r\n                })\r\n                .catch(() =>{\r\n                    this.isEmptyEnrollments = true\r\n                })\r\n        },\r\n\r\n        getClass(element){\r\n            var className = (element !== '') ? 'has-error' : '';\r\n            return className;\r\n        },\r\n\r\n        getListStudents(){\r\n            Students.listStudents().then(response => {\r\n                this.students = response.data.data\r\n            })\r\n        },\r\n\r\n        findObjectByKey(array, key, value) {\r\n            for (var i = 0; i < array.length; i++) {\r\n                var field = (key === 'student_id') ? array[i].student_id : array[i].course_id\r\n                if (field === value) {                   \r\n                   return array[i];\r\n                }\r\n            }\r\n            return null;\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    form, input, label {\r\n        font-family: 'Poppins';\r\n    }\r\n    .btn{\r\n        margin: 5px;\r\n    }\r\n    .btn-detail {\r\n        color: #649cb1 !important;\r\n        border: .05rem solid #649cb1 !important;\r\n        opacity: 1 !important;\r\n        width: 200px;\r\n    }\r\n    .btn-detail:hover {\r\n        color: #fff !important;\r\n        background: #649cb1 !important;\r\n        border: .05rem solid #fff !important;\r\n        opacity: 1 !important;\r\n        width: 200px;\r\n    }\r\n    .btn-edit {\r\n        color: #41b883 !important;\r\n        border: .05rem solid #41b883 !important;\r\n        opacity: 1 !important;\r\n        width: 200px;\r\n    }\r\n    .btn-edit:hover {\r\n        color: #fff !important;\r\n        background: #41b883 !important;\r\n        border: .05rem solid #fff !important;\r\n        opacity: 1 !important;\r\n        width: 200px;\r\n    }\r\n\r\n    .title-h {\r\n        color: #41b883;\r\n        font-family: fantasy;\r\n    }\r\n\r\n    .filter-body{\r\n        padding: .4rem;\r\n        color: #3b4351;        \r\n    }\r\n    .card{\r\n        box-shadow: 0 0.25rem 1rem rgba(48, 55, 66, 0.15);\r\n        border:0px;\r\n    }\r\n</style>"],"sourceRoot":"src/pages/courses"}]}